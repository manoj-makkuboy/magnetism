<?xml version="1.0" ?>
<!-- note this file must actually be XML to make XSLT work -->
<html xmlns:dh="http://dumbhippo.com/schema/clientjs">
    <head>
        <dh:coreStyle/>
        <dh:notificationStyle/>
        <title>dumbhippo notification</title>
        <dh:coreScript/>
        <dh:notificationScript/>           
    </head>
    <body onload="dhInit();">
        <div id="dh-notification-leftside">
            <div id="dh-notification-photo"></div>
            <div id="dh-notification-photolink"></div>
            <div id="dh-notification-navigation">
                <div id="dh-notification-navigation-text"></div>
                <div id="dh-notification-navigation-buttons"></div>
            </div>
        </div>
        <div id="dh-notification-rightside">
            <div id="dh-notification-title"></div>
            <div id="dh-notification-body"></div>
            <div id="dh-notification-bottomright">
                <div id="dh-notification-viewers-outer">
                    <div id="dh-notification-viewers-bubble">
                        <div id="dh-notification-viewers-photo"></div>
                        <span id="dh-notification-viewers"></span>
                    </div>
                </div>
                <div id="dh-notification-meta-outer">
                    <span id="dh-notification-meta"></span>
                </div>
            </div>
        </div>
        <dh:closeButton />
    </body>    
</html>
