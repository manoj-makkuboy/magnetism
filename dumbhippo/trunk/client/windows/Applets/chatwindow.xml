<?xml version="1.0" ?>
<!-- note this file must actually be XML to make XSLT work -->
<html xmlns:dh="http://dumbhippo.com/schema/clientjs">
    <head>
        <dh:coreStyle/>
        <dh:chatStyle/>
        <title>DumbHippo Chat Room</title>
        <dh:coreScript/>
        <dh:chatScript/>
    </head>
    <body scroll="no">
        <div id="dhChatPeopleDiv"></div>
        <div id="dhChatAdsDiv">
            <div id="dhChatAdsInnerDiv">
                Make money fast.<br/>
                Get a green card today.<br/>
            </div>
        </div>
        <div id="dhChatMessagesDiv"></div>
        <textarea id="dhChatMessageInput" cols="40" rows="3"></textarea>
        <input id="dhChatSendButton" type="button" value="Send" onclick="dh.display.sendClicked()"></input>
    </body>
</html>
