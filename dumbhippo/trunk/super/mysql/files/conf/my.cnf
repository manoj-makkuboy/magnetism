[client]
user=root
host=127.0.0.1
port=@@mysqlPort@@
password=@@dbPassword@@

[mysqld]
datadir=@@targetdir@@/data/
socket=@@targetdir@@/run/mysql.sock
port=@@mysqlPort@@
#log=@@targetdir@@/log/queries.log
log_slow_queries=@@targetdir@@/log/slow-queries.log
long_query_time=1
     
[mysqld_safe]
err-log=@@targetdir@@/log/mysqld.log
pid-file=@@targetdir@@/run/mysqld.pid

